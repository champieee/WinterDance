<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>


<StackPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="600.0" minWidth="600.0" prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo2.WinterDanceController">
   <children>
      <Label text="Winter Dance" underline="true" StackPane.alignment="TOP_CENTER">
         <StackPane.margin>
            <Insets top="150.0" />
         </StackPane.margin>
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <TextField fx:id="name" maxWidth="300.0" promptText="Name:">
         <StackPane.margin>
            <Insets bottom="50.0" />
         </StackPane.margin>
      </TextField>
      <TextField fx:id="ticket" maxWidth="300.0" promptText="Ticket Number:">
         <StackPane.margin>
            <Insets top="50.0" />
         </StackPane.margin>
      </TextField>
      <Button fx:id="enter" mnemonicParsing="false" onAction="#checkIn" text="Enter" StackPane.alignment="BOTTOM_CENTER">
         <StackPane.margin>
            <Insets bottom="150.0" />
         </StackPane.margin>
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button fx:id="checkInList" mnemonicParsing="false" text="Checked In" StackPane.alignment="BOTTOM_LEFT">
         <StackPane.margin>
            <Insets bottom="75.0" left="200.0" />
         </StackPane.margin>
      </Button>
      <Button fx:id="checkOutList" mnemonicParsing="false" text="Checked Out" StackPane.alignment="BOTTOM_RIGHT">
         <StackPane.margin>
            <Insets bottom="75.0" right="200.0" />
         </StackPane.margin>
      </Button>
   </children>
</StackPane>
